{
  "jdbcSink": {
    "driver": "com.mysql.cj.jdbc.Driver",
    "url": "jdbc:mysql://47.103.79.201:3306/mallbigdata_us",
    "username": "mytest",
    "password": "123456@"
  },
  "syncConfig": {
    "minSyncBatchSize": 40
  },
  "extractors": [
    {"id": 1, "urlPattern": ".+/dp/.+",                  "minContentSize": 500000, "minNumNonBlankFields": 20,
      "sqlTemplateResource": "x-asin.sql",                    "sinkTableName": "asin_sync_utf8mb4"},

    {"id": 2, "urlPattern": ".+/seller/.+",              "minContentSize": 100000, "minNumNonBlankFields": 8,
      "sqlTemplateResource": "x-sellers.sql",                 "sinkTableName": "seller_sync"},

    {"id": 3, "urlPattern": ".+/product-reviews/.+",     "minContentSize": 100000, "minNumNonBlankFields": 10,
      "sqlTemplateResource": "x-product-reviews.sql",         "sinkTableName": "asin_review_sync"},

    {"id": 4, "urlPattern": ".+/zgbs/.+",                "minContentSize": 100000, "minNumNonBlankFields": 5,
      "sqlTemplateResource": "x-asin-best-sellers.sql",       "sinkTableName": "asin_best_sellers_sync"},

    {"id": 5, "urlPattern": ".+/new-releases/.+",        "minContentSize": 100000, "minNumNonBlankFields": 5,
      "sqlTemplateResource": "x-asin-new-releases.sql",       "sinkTableName": "asin_new_releases_sync"},

    {"id": 6, "urlPattern": ".+/movers-and-shakers/.+",  "minContentSize": 100000, "minNumNonBlankFields": 5,
      "sqlTemplateResource": "x-asin-movers-and-shakers.sql", "sinkTableName": "asin_movers_and_shakers_sync"},

    {"id": 7, "urlPattern": ".+/most-wished-for/.+",     "minContentSize": 100000, "minNumNonBlankFields": 5,
      "sqlTemplateResource": "x-asin-most-wished-for.sql",    "sinkTableName": "asin_most_wished_for_sync"},


    {"id": 100, "urlPattern": ".+/dp/.+",     "minContentSize": 100000, "minNumNonBlankFields": 8,
      "sqlTemplateResource": "x-asin-ad-also-view.sql",    "sinkTableName": "asin_ad_sync", "parentId":  1},

    {"id": 101, "urlPattern": ".+/dp/.+",     "minContentSize": 100000, "minNumNonBlankFields": 8,
      "sqlTemplateResource": "x-asin-ad-similar.sql",    "sinkTableName": "asin_ad_sync", "parentId":  1},

    {"id": 102, "urlPattern": ".+/dp/.+",     "minContentSize": 100000, "minNumNonBlankFields": 8,
      "sqlTemplateResource": "x-asin-ad-sponsored.sql",    "sinkTableName": "asin_ad_sync", "parentId":  1}
  ]
}
